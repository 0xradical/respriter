<div class='l-grid[5w@phone]'>
  <% %w(udemy coursera kuadro udacity alura edx descomplica treehouse eduk
  khan-academy babbel memrise general-assembly masterclass tuts+ code-school desire2learn
  codecademy datacamp platzi).each do |p| %>
    <div class='l-col' style='max-height:90px'>
      <div style='max-width:72px;text-align:center'>
        <div style='padding:15px;padding-bottom:10px;'>
          <%= svg_providers_lib(p, { width: '24px', height: '24px'} ) %>
        </div>
        <h6 class='mx-D(n)@phone mx-D(n)@tablet' style='margin-top:0px;word-wrap:break-word'>
          <%= p.split('-').map(&:capitalize).join(' ') %>
        </h6>
      </div>
    </div>
  <% end %>
</div>
