#!/usr/bin/env sh

case `echo $1 | sed "s/-dev$//g" | sed "s/[-_]/./g"` in
base)
  echo "napoleon.clspt"
  exit 0
  ;;
app)
  echo "napoleon.clspt"
  exit 0
  ;;
anonymous.proxy)
  echo "anonymous.proxy.clspt"
  exit 0
  ;;
api)
  echo "api.napoleon.clspt"
  exit 0
  ;;
database)
  echo "database.napoleon.clspt"
  exit 0
  ;;
test.database)
  echo "database.napoleon.test"
  exit 0
  ;;
heavyload.service)
  echo "heavyload.service.test"
  exit 0
  ;;
test.heavyload.service)
  echo "heavyload.service.test"
  exit 0
  ;;
mail)
  echo "mail.clspt"
  exit 0
  ;;
mitm)
  echo "mitm.proxy.clspt"
  exit 0
  ;;
mitm.proxy)
  echo "mitm.proxy.clspt"
  exit 0
  ;;
napoleon)
  echo "napoleon.clspt"
  exit 0
  ;;
test)
  echo "napoleon.test"
  exit 0
  ;;
test.napoleon)
  echo "napoleon.test"
  exit 0
  ;;
provider)
  echo "provider.clspt"
  exit 0
  ;;
proxy)
  echo "proxy.clspt"
  exit 0
  ;;
test.proxy)
  echo "proxy.test"
  exit 0
  ;;
s3)
  echo "s3.clspt"
  exit 0
  ;;
worker)
  echo "worker.napoleon.clspt"
  exit 0
  ;;
*)
  echo $1 | sed "s/[-_]/./g"
  ;;
esac
